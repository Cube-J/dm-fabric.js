  "homepage": "http://fabricjs.com/",
  "version": "5.1.5",
  "author": "Juriy Zaytsev <kangax@gmail.com>",
  "contributors": [
    {
      "name": "Andrea Bogazzi",
      "email": "andreabogazzi79@gmail.com"
    },
    {
      "name": "Steve Eberhardt",
